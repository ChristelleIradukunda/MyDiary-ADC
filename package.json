  },
  "homepage": "https://github.com/ChristelleIradukunda/MyDiary-ADC#readme",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
